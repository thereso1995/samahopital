{% extends 'base.html.twig' %}

{% block title %}Hello MedecinController!{% endblock %}

{% block body %}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Matricule</th>
      <th scope="col">Prenom</th>
      <th scope="col">Nom</th>
      <th scope="col">Telephone</th>
      <th scope="col">Datenaiss</th>
      <th scope="col">Email</th>
      <th scope="col">Service</th>
      <th scope="col">Specialite</th>
    </tr>
  </thead>
  <tbody>
            {% for medecin in medecins %}
           
            <tr>
                
                <td>{{medecin.id}}</td>
                <td>{{medecin.matricule}}</td>
                <td>{{medecin.prenom}}</td>
                <td>{{medecin.nom}}</td>
                <td>{{medecin.tel}}</td>
                <td>{{medecin.datenaiss}}</td>
                <td>{{medecin.email}}</td>
                <td>{{medecin.service.libelle}}</td>
                
                

                
                
                <th scope="row"></th>
              </tr>
            {% endfor %}
    
    </tbody>
</table>
{% endblock %}
